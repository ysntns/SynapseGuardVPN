<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="120dp"
    android:height="120dp"
    android:viewportWidth="120"
    android:viewportHeight="120">

    <!-- Shield background -->
    <path
        android:fillColor="#00D9FF"
        android:fillAlpha="0.2"
        android:pathData="M60,10 L100,30 L100,70 Q100,90 60,110 Q20,90 20,70 L20,30 Z" />

    <!-- Shield outline -->
    <path
        android:strokeColor="#00D9FF"
        android:strokeWidth="2"
        android:fillColor="@android:color/transparent"
        android:pathData="M60,10 L100,30 L100,70 Q100,90 60,110 Q20,90 20,70 L20,30 Z" />

    <!-- Brain/circuit pattern inside shield -->
    <!-- Left hemisphere -->
    <path
        android:strokeColor="#00F0FF"
        android:strokeWidth="1.5"
        android:fillColor="@android:color/transparent"
        android:pathData="M45,40 Q40,50 45,60 Q50,70 55,65" />

    <!-- Right hemisphere -->
    <path
        android:strokeColor="#00F0FF"
        android:strokeWidth="1.5"
        android:fillColor="@android:color/transparent"
        android:pathData="M75,40 Q80,50 75,60 Q70,70 65,65" />

    <!-- Central connection -->
    <path
        android:strokeColor="#00D9FF"
        android:strokeWidth="2"
        android:fillColor="@android:color/transparent"
        android:pathData="M55,65 L65,65" />

    <!-- Neural nodes (circles drawn as paths) -->
    <!-- Node at (45, 40) radius 2 -->
    <path
        android:fillColor="#00F0FF"
        android:pathData="M47,40 A2,2 0 1,1 43,40 A2,2 0 1,1 47,40 Z" />

    <!-- Node at (75, 40) radius 2 -->
    <path
        android:fillColor="#00F0FF"
        android:pathData="M77,40 A2,2 0 1,1 73,40 A2,2 0 1,1 77,40 Z" />

    <!-- Center node at (60, 65) radius 3 -->
    <path
        android:fillColor="#00D9FF"
        android:pathData="M63,65 A3,3 0 1,1 57,65 A3,3 0 1,1 63,65 Z" />

    <!-- Node at (45, 60) radius 2 -->
    <path
        android:fillColor="#00F0FF"
        android:pathData="M47,60 A2,2 0 1,1 43,60 A2,2 0 1,1 47,60 Z" />

    <!-- Node at (75, 60) radius 2 -->
    <path
        android:fillColor="#00F0FF"
        android:pathData="M77,60 A2,2 0 1,1 73,60 A2,2 0 1,1 77,60 Z" />

    <!-- Lock symbol at bottom -->
    <path
        android:fillColor="#00D9FF"
        android:pathData="M52,75 L52,82 L68,82 L68,75 Q68,70 60,70 Q52,70 52,75 Z" />

    <path
        android:strokeColor="#00D9FF"
        android:strokeWidth="1.5"
        android:fillColor="@android:color/transparent"
        android:pathData="M55,75 L55,72 Q55,68 60,68 Q65,68 65,72 L65,75" />

    <!-- VPN connection indicators -->
    <path
        android:strokeColor="#00F0FF"
        android:strokeWidth="1"
        android:strokeLineCap="round"
        android:pathData="M30,50 L35,50" />

    <path
        android:strokeColor="#00F0FF"
        android:strokeWidth="1"
        android:strokeLineCap="round"
        android:pathData="M85,50 L90,50" />

    <path
        android:strokeColor="#00F0FF"
        android:strokeWidth="1"
        android:strokeLineCap="round"
        android:pathData="M60,20 L60,25" />

</vector>
