<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="120dp"
    android:height="120dp"
    android:viewportWidth="120"
    android:viewportHeight="120">

    <!-- Shield background -->
    <path
        android:fillColor="#00D9FF"
        android:fillAlpha="0.2"
        android:pathData="M60,10 L100,30 L100,70 Q100,90 60,110 Q20,90 20,70 L20,30 Z" />

    <!-- Shield outline -->
    <path
        android:strokeColor="#00D9FF"
        android:strokeWidth="2"
        android:fillColor="@android:color/transparent"
        android:pathData="M60,10 L100,30 L100,70 Q100,90 60,110 Q20,90 20,70 L20,30 Z" />

    <!-- Brain/circuit pattern inside shield -->
    <!-- Left hemisphere -->
    <path
        android:strokeColor="#00F0FF"
        android:strokeWidth="1.5"
        android:fillColor="@android:color/transparent"
        android:pathData="M45,40 Q40,50 45,60 Q50,70 55,65" />

    <!-- Right hemisphere -->
    <path
        android:strokeColor="#00F0FF"
        android:strokeWidth="1.5"
        android:fillColor="@android:color/transparent"
        android:pathData="M75,40 Q80,50 75,60 Q70,70 65,65" />

    <!-- Central connection -->
    <path
        android:strokeColor="#00D9FF"
        android:strokeWidth="2"
        android:fillColor="@android:color/transparent"
        android:pathData="M55,65 L65,65" />

    <!-- Neural nodes -->
    <circle
        android:fillColor="#00F0FF"
        android:cx="45"
        android:cy="40"
        android:radius="2" />

    <circle
        android:fillColor="#00F0FF"
        android:cx="75"
        android:cy="40"
        android:radius="2" />

    <circle
        android:fillColor="#00D9FF"
        android:cx="60"
        android:cy="65"
        android:radius="3" />

    <circle
        android:fillColor="#00F0FF"
        android:cx="45"
        android:cy="60"
        android:radius="2" />

    <circle
        android:fillColor="#00F0FF"
        android:cx="75"
        android:cy="60"
        android:radius="2" />

    <!-- Lock symbol at bottom -->
    <path
        android:fillColor="#00D9FF"
        android:pathData="M52,75 L52,82 L68,82 L68,75 Q68,70 60,70 Q52,70 52,75 Z" />

    <path
        android:strokeColor="#00D9FF"
        android:strokeWidth="1.5"
        android:fillColor="@android:color/transparent"
        android:pathData="M54,75 L54,72 Q54,68 60,68 Q66,68 66,72 L66,75" />

</vector>
